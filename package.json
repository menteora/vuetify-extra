  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve ./dev/main.js",
    "build:demo": "vue-cli-service build --dest demo ./dev/main.js",
    "build": "vue-cli-service build --target lib --name menteora src/plugin.js",
    "build:local": "vue-cli-service build --target lib --name menteora src/plugin.js && npm pack",
