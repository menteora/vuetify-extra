{
  "name": "@menteora/vuetify-extra",
  "version": "1.0.0-beta.0",
  "license": "MIT",
  "main": "dist/vuetify-extra.ssr.js",
  "browser": "dist/vuetify-extra.esm.js",
  "module": "dist/vuetify-extra.esm.js",
  "unpkg": "dist/vuetify-extra.min.js",
  "scripts": {
    "serve": "vue-cli-service serve ./dev/main.js",
    "build:demo": "vue-cli-service build --dest demo ./dev/main.js",
    "lint": "vue-cli-service lint --fix && vue-cli-service lint dev --fix",
    "deploy:demo": "vue-cli-service build --dest demo ./dev/main.js && ./deploy.demo.sh",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "build:local": "cross-env NODE_ENV=production rollup --config build/rollup.config.js && npm pack"
  },
  "files": [
    "dist",
    "src/**/*.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/menteora/vuetify-extra"
  },
  "peerDependencies": {
    "vue": "*",
    "vuetify": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.5.7",
    "@vue/cli-service": "^4.5.7",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.10.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.1",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.2",
    "vue": "^2.6.12",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-json-tree": "^0.4.3",
    "vue-json-tree-view": "^2.1.6",
    "vue-template-compiler": "^2.6.12",
    "vuetify": "^2.3.13",
    "vuetify-loader": "^1.6.0"
  }
}
